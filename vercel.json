{
  "buildCommand": "pnpm build && pnpm build:docs",
  "outputDirectory": "docs-site/dist",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "rewrites": [
    {
      "source": "/r/:path*",
      "destination": "/api/r?file=:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
